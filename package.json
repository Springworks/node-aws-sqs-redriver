{
  "name": "aws-sqs-redriver",
  "description": "Re-drives messages from one SQS queue to another, e.g. to re-process \"dead letters\"",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --cache .",
    "build": "rm -rf build && babel src --out-dir build",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test istanbul cover _mocha",
    "test-no-cov": "NODE_ENV=test mocha",
    "test-acceptance": "NODE_ENV=test istanbul cover _mocha -- --fgrep '/acceptance/'",
    "test-component": "NODE_ENV=test istanbul cover _mocha -- --fgrep '/component/'",
    "test-unit": "NODE_ENV=test istanbul cover _mocha -- --fgrep '/unit/'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Springworks/node-aws-sqs-redriver.git"
  },
  "keywords": [
    "aws",
    "sqs",
    "redrive",
    "lambda"
  ],
  "author": "Springworks <hello@springworks.se> (http://www.springworks.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Springworks/node-aws-sqs-redriver/issues"
  },
  "homepage": "https://github.com/Springworks/node-aws-sqs-redriver#readme",
  "devDependencies": {
    "@springworks/test-harness": "1.5.2",
    "aws-sdk": "^2.7.21",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-transform-strict-mode": "6.18.0",
    "babel-preset-es2015-node4": "2.1.1",
    "babel-register": "6.18.0",
    "eslint": "3.13.1",
    "eslint-config-springworks": "7.0.7",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-mocha": "4.8.0",
    "eslint-plugin-should-promised": "2.0.0",
    "eslint-plugin-springworks": "2.0.1",
    "istanbul": "0.4.5",
    "lodash.clonedeep": "4.5.0",
    "mocha": "3.2.0",
    "semantic-release": "6.3.2"
  },
  "dependencies": {
    "@springworks/logger-factory": "3.4.0",
    "config": "1.24.0",
    "@springworks/input-validator": "5.0.1"
  }
}
